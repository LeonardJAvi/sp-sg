<%= simple_form_for (@order),url: :create_report_admin_orders, method: :post do |f| %>
	
		<%=f.input :stack_state_id, collection: StackState.all,input_html: {style: "margin-botton:px" } %>
		<%=f.input :project_id, collection: Project.all,input_html: {style: "margin-botton:px" } %>
		<%=f.input :date_start_real, as: :string, input_html: {class: "datepicker" , style: "margin-botton:px" }%>
    	<%=f.input :date_end_real, as: :string, input_html: {class: "datepicker" , style: "margin-botton:px" }%>
 	   <div class="actions"><%= f.submit "Search" %></div>
<%end%>
<%if $bandera == "1"%>
	<%if ($parametro.date_start_real.to_s == "") or ($parametro.date_end_real.to_s == "")%>
		<%$date_start = $parametro.date_start_real = "11/11/1111"%></br>
		<%$date_end = $parametro.date_end_real = "11/11/1111"%></br>
	<%end%>

	<%@orders.each do |order|%>
		<%if $parametro.stack_state == order.stack_state%>
			<%if $parametro.project == order.project%>
				<%if (Date.parse (order.date_start_real.strftime("%d-%m-%Y"))) >= (Date.parse($parametro.date_start_real.strftime("%d-%m-%Y"))) and (Date.parse(order.date_end_real.strftime("%d-%m-%Y"))) <= (Date.parse($parametro.date_end_real.strftime("%d-%m-%Y")))%>
					<%="si status, si  project, si la fecha  #{order.id}"%></br>
				
				<%elsif ($parametro.date_start_real.to_s == $date_start) or ($parametro.date_end_real.to_s == $date_end)%>
					<%="si status, si  project, no la fecha  #{order.id}"%></br>
				<%end%>
			

			<%elsif $parametro.project == nil%>
